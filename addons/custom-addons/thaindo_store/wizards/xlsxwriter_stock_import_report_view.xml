<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <record id="wizard_xlsx_stock_import_report_view" model="ir.ui.view">
       <field name="name">Stock Import</field>
       <field name="model">xlsx.stock.import.report.wizard</field>
       <field name="arch" type="xml">
            <form string="Report Options">
                <group>
                    <group>
                           <field name="date_start"/>
                           
                    </group>
                    <group>
                           <field name="date_end" required="1"/>
                           <field name="categ_ids" widget="many2many_tags" options="{'no_create': True, 'no_create_edit':True, 'no_open': True}"/>
                    </group>
                </group>
                <footer>
                   <button name="print_xlsx" string="PRINT" type="object"
                             default_focus="1" class="oe_highlight"/>
                   <button string="Cancel" class="btn btn-default" special="cancel"/>
                </footer>
           </form>
       </field>
   </record>

   <record id="action_xlsx_stock_import_report_wizard" model="ir.actions.act_window">
       <field name="name">รายงานรายการนำเข้า</field>
       <field name="res_model">xlsx.stock.import.report.wizard</field>
       <field name="view_mode">form</field>
       <field name="target">new</field>
   </record>

   <menuitem id="menu_stock_import_report" parent="menu_stock_report"
            action="action_xlsx_stock_import_report_wizard" sequence="3" />
</odoo>
